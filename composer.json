{
    "name": "cpliakas/php-project-starter",
    "type": "library",
    "description": "A command line tool that allows developers to quickly create PHP applications that use common conventions and best-in-breed development tools.",
    "keywords": ["php"],
    "homepage": "https://github.com/cpliakas/php-project-starter",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Pliakas",
            "email": "opensource@chrispliakas.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "cpliakas/git-wrapper": "~1.0",
        "guzzle/http": ">=3.9.0,<4.0.0",
        "herrera-io/phar-update": "~1.0",
        "symfony/console": "~2.0",
        "symfony/filesystem": "~2.0"
    },
    "require-dev": {
        "guzzle/plugin-mock": ">=3.9.0,<4.0.0",
        "phpunit/phpunit": "~3.0",
        "scrutinizer/ocular": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Cpliakas\\PhpProjectStarter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cpliakas\\PhpProjectStarter\\Test\\": "test/"
        }
    },
    "bin": ["bin/php-project.php", "bin/php-project"]
}
